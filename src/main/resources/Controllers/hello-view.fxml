<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.media.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <MediaView fx:id="mediaView" visible="false" />
   <Slider fx:id="sliderVolume" />
   <Button mnemonicParsing="false" onAction="#onFileButtonPressed" text="File Choose" />
   <Button fx:id="repeat" mnemonicParsing="false" onAction="#onRepeatButtonPressed" text="R1" />

    <Label fx:id="welcomeText" />
    <Button fx:id="pausePlay" onAction="#onPauseButtonPressed" text="Play!" />
</VBox>
